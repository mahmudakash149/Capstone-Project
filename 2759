#include <stdio.h>

int main() {
    char A, B, C;

    // Read input characters
    scanf(" %c", &A);
    scanf(" %c", &B);
    scanf(" %c", &C);

    // Print output in the required format
    printf("A = %c, B = %c, C = %c\n", A, B, C);
    printf("A = %c, B = %c, C = %c\n", B, C, A);
    printf("A = %c, B = %c, C = %c\n", C, A, B);

    return 0;
}
